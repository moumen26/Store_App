{
  "expo": {
    "name": "Mosagro",
    "slug": "Mosagro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Light.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to scan barcodes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Dark.png",
        "backgroundColor": "#192130"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.mosagro.clientapplication",
      "jsEngine": "hermes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "We need access to your camera to scan barcodes."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/Dark.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#192130"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2bd7bfe9-1c36-452d-876e-f3ba161f8850"
      }
    }
  }
}
